{
  "name": "@helios/root",
  "private": true,
  "version": "1.0.0",
  "description": "Helios Plugin Workspace - Multiple React apps for Apache Airflow",
  "scripts": {
    "build": "pnpm -r build",
    "build:env": "pnpm --filter environment-manager build",
    "build:alerts": "pnpm --filter alerts build",
    "build:alerts-tab": "pnpm --filter alerts-tab build",
    "build:astro-bar": "pnpm --filter astro-bar build",
    "dev:env": "pnpm --filter environment-manager dev",
    "dev:alerts": "pnpm --filter alerts dev",
    "dev:alerts-tab": "pnpm --filter alerts-tab dev",
    "dev:astro-bar": "pnpm --filter astro-bar dev",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
  },
  "keywords": [
    "airflow",
    "plugin",
    "react"
  ],
  "author": "Apache Airflow",
  "license": "Apache-2.0",
  "dependencies": {
    "@fontsource/inter": "^5.2.8"
  }
}
